<% if notice %>
  <div class="alert alert-success alert-dismissible" >
    <button type="button" class="close" data-dismiss="alert"><span>&times;</span></button>
    <%= notice %>
  </div>
<% elsif alert %>
  <div class="alert alert-danger alert-dismissible" >
    <button type="button" class="close" data-dismiss="alert"><span>&times;</span></button>
    <%= alert %>
  </div>
<% else %>
  <div></div>
<% end %>
<div class="ajax-flash-display" ></div>
